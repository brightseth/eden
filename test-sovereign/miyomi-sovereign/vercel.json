{
  "name": "miyomi-sovereign",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "AGENT_HANDLE": "miyomi",
    "AGENT_NAME": "MIYOMI"
  }
}